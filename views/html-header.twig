<head>
    <meta charset="{{site.charset}}" />
    <title>   
        {% if wp_title %}
        {{ wp_title }} - {{ site.name }}
        {% else %}
        {{ site.name }}
        {% endif %}
    </title>
    <meta name="description" content="{{site.description}}">

    {% if post.thumbnail %}
      <meta property="og:image" content="{{post.thumbnail.src|resize(1500)}}"/>
    {% else %}
      <meta property="og:image" content="http://belka.us/facebook.jpg"/>
    {% endif %}

    <!-- all the css is here and is imported from other files -->
    <link rel="stylesheet" href="{{site.theme.link}}/style.css" type="text/css" media="screen" />
    
    <!-- page basic needs -->
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    
    <!-- mobile needs -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{site.pingback_url}}" />

